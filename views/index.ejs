    <!-- Table Card -->
    <div class="table-card">
      <div class="card-header">
        <h2>Data Capaian Detail</h2>
        <div style="display: flex; gap: 10px; align-items: center; flex-wrap: wrap;">
          <div class="sort-controls">
            <label for="sortSelect" style="font-size: 13px; color: var(--text-muted); margin-right: 8px;">Urutkan:</label>
            <select id="sortSelect" class="sort-select" onchange="sortTable()">
              <option value="">Pilih Sorting</option>
              <option value="persentase-desc">Persentase Tertinggi → Terendah</option>
              <option value="persentase-asc">Persentase Terendah → Tertinggi</option>
              <option value="fakultas-asc">Fakultas A → Z</option>
              <option value="fakultas-desc">Fakultas Z → A</option>
              <option value="prodi-asc">Program Studi A → Z</option>
              <option value="prodi-desc">Program Studi Z → A</option>
            </select>
          </div>
          <button class="btn btn-small" onclick="exportData()">Export</button>
          <button class="btn btn-small" onclick="refreshData()">Refresh</button>
        </div>
      </div>
      
      <div class="table-container">
        <table class="data-table" id="dataTable">
          <thead>
            <tr>
              <th>Fakultas</th>
              <th>Program Studi</th>
              <th>Capaian Rata-rata (%)</th>
              <th>Jumlah Alumni</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody id="tableBody">
            <% if (data && data.length > 0) { %>
              <% data.forEach(function(item){ %>
                <% 
                  const jumlahAlumni = item.jumlah_alumni || 0;
                  // Pastikan capaian_rata selalu di-parse dengan benar
                  let capaianRata = 0;
                  if (jumlahAlumni > 0 && item.capaian_rata !== null && item.capaian_rata !== undefined && item.capaian_rata !== '') {
                    const parsed = parseFloat(item.capaian_rata);
                    capaianRata = isNaN(parsed) ? 0 : parsed;
                  }
                  
                  const colorClass = jumlahAlumni === 0 ? '' : (capaianRata >= 75 ? 'green' : capaianRata >= 50 ? 'yellow' : 'red');
                  const statusClass = jumlahAlumni === 0 ? '' : (capaianRata >= 75 ? 'success' : capaianRata >= 50 ? 'warning' : 'danger');
                  const statusText = jumlahAlumni === 0 ? '-' : (capaianRata >= 75 ? 'Mencapai Target' : capaianRata >= 50 ? 'Mendekati Target' : 'Perlu Peningkatan');
                %>
                <tr data-fakultas="<%= item.fakultas %>" data-prodi="<%= item.prodi %>" data-persentase="<%= capaianRata %>">
                  <td><%= item.fakultas %></td>
                  <td><%= item.prodi %></td>
                  <td>
                    <div class="progress-bar">
                      <div 
                        class="progress-fill <%= colorClass %>" 
                        data-width="<%= capaianRata %>">
                      </div>
                      <span class="progress-text"><%= capaianRata %>%</span>
                    </div>
                  </td>
                  <td><%= jumlahAlumni %></td>
                  <td>
                    <% if (jumlahAlumni === 0) { %>
                      <span class="status-badge">-</span>
                    <% } else { %>
                      <span class="status-badge status-<%= statusClass %>"><%= statusText %></span>
                    <% } %>
                  </td>
                </tr>
              <% }) %>
            <% } else { %>
              <tr><td colspan="5" class="no-data">Tidak ada data capaian.</td></tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
