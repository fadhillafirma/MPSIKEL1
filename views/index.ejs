          resizedCanvas.width = newWidth;
          resizedCanvas.height = newHeight;
          const resizedCtx = resizedCanvas.getContext('2d');
          
          // Gunakan imageSmoothingEnabled untuk kualitas lebih baik
          resizedCtx.imageSmoothingEnabled = true;
          resizedCtx.imageSmoothingQuality = 'high';
          
          resizedCtx.drawImage(canvas, 0, 0, newWidth, newHeight);
          
          // Gunakan canvas yang sudah di-resize
          canvas = resizedCanvas;
          console.log('Canvas resized successfully to:', newWidth, 'x', newHeight);
        }

        // Remove temporary container
        if (exportContainer && exportContainer.parentNode) {
          document.body.removeChild(exportContainer);
          exportContainer = null;
        }

        // Convert canvas to image dengan error handling yang lebih baik
        let imgData;
        try {
          // Coba dengan kualitas tinggi dulu
          imgData = canvas.toDataURL('image/png', 1.0);
          
          // Validasi hasil
          if (!imgData || imgData === 'data:,') {
            // Coba dengan kualitas lebih rendah
            console.warn('Konversi dengan kualitas tinggi gagal, mencoba kualitas lebih rendah...');
            imgData = canvas.toDataURL('image/jpeg', 0.9);
            
            if (!imgData || imgData === 'data:,') {
              // Coba dengan kualitas lebih rendah lagi
              imgData = canvas.toDataURL('image/jpeg', 0.7);
              
              if (!imgData || imgData === 'data:,') {
                throw new Error('Gagal mengkonversi canvas ke gambar setelah beberapa percobaan.');
              }
            }
          }
          
          console.log('Image data length:', imgData.length, 'characters');
        } catch (e) {
          console.error('Error converting canvas:', e);
          
          // Coba metode alternatif dengan blob
          try {
            await new Promise((resolve, reject) => {
              canvas.toBlob((blob) => {
                if (!blob) {
                  reject(new Error('Gagal mengkonversi canvas ke blob. Canvas mungkin terlalu besar atau browser tidak mendukung.'));
                  return;
                }
                
                const reader = new FileReader();
                reader.onload = () => {
                  imgData = reader.result;
                  resolve(imgData);
                };
                reader.onerror = () => {
                  reject(new Error('Gagal membaca blob.'));
                };
                reader.readAsDataURL(blob);
              }, 'image/png', 0.9);
            });
          } catch (blobError) {
            throw new Error('Gagal mengkonversi canvas: ' + (e.message || e.toString()) + '. Error blob: ' + (blobError.message || blobError.toString()) + '. Silakan coba refresh halaman atau kurangi jumlah data.');
          }
        }

        // Create PDF using jsPDF
        if (typeof window.jspdf === 'undefined') {
          alert('Library PDF tidak dimuat. Silakan refresh halaman.');
          btn.disabled = false;
          btn.innerHTML = originalText;
          return;
        }

        const { jsPDF } = window.jspdf;
        const pdf = new jsPDF('p', 'mm', 'a4');
        
        const pageWidth = 210; // A4 width in mm
        const pageHeight = 297; // A4 height in mm
        const margin = 15; // Perbesar margin untuk spacing lebih baik
        const contentWidth = pageWidth - (margin * 2);
        
        // Fungsi untuk menambahkan header ke setiap halaman - Rapikan spacing
        function addPageHeader(pdf, pageNum, totalPages) {
          pdf.setFontSize(36); // Rapikan ukuran
          pdf.setFont('helvetica', 'bold');
          pdf.setTextColor(47, 107, 49);
          pdf.text('Riwayat Capaian IKU-1', margin, 20);
          
          pdf.setFontSize(16); // Rapikan ukuran
          pdf.setFont('helvetica', 'normal');
          pdf.setTextColor(0, 0, 0);
          pdf.text('Career Development Center Universitas Andalas', margin, 28);
          
          pdf.setFontSize(12); // Rapikan ukuran
          pdf.setTextColor(100, 100, 100);
          pdf.text(`Tanggal: ${new Date().toLocaleDateString('id-ID', { 
            weekday: 'long', 
            year: 'numeric', 
            month: 'long', 
            day: 'numeric' 
          })}`, margin, 36);
          
          // Tambahkan garis pemisah
          pdf.setDrawColor(200, 200, 200);
          pdf.setLineWidth(0.5);
          pdf.line(margin, 40, pageWidth - margin, 40);
          
          // Tambahkan nomor halaman
          pdf.setFontSize(11); // Rapikan ukuran
          pdf.setTextColor(150, 150, 150);
          pdf.text(`Halaman ${pageNum} dari ${totalPages}`, pageWidth - margin - 30, pageHeight - 10, { align: 'right' });
        }

        // Calculate image dimensions - Perbesar ukuran maksimal
        const imgAspectRatio = canvas.width / canvas.height;
        const imgWidth = contentWidth; // Gunakan full width
        const imgHeight = imgWidth / imgAspectRatio;
        
        // Calculate available height per page - Rapikan spacing
        const titleHeight = 45; // Tinggi header - disesuaikan dengan header baru
        const footerHeight = 12; // Tinggi footer - rapikan
        // Tambahkan safety margin SANGAT BESAR untuk mencegah potongan baris tabel
        // Safety margin besar untuk memastikan tidak ada nama fakultas/prodi yang terpotong
        const safetyMargin = 20; // Margin keamanan SANGAT BESAR untuk mencegah potongan
        const maxHeightPerPage = pageHeight - titleHeight - margin - footerHeight - safetyMargin;
        
        // Hitung berapa halaman yang dibutuhkan
        const totalPages = Math.ceil(imgHeight / maxHeightPerPage);
        
        console.log('Total pages needed:', totalPages);
        console.log('Image height:', imgHeight, 'mm');
        console.log('Max height per page:', maxHeightPerPage, 'mm');
        console.log('Content width:', contentWidth, 'mm');
        
        // Split gambar ke multiple pages menggunakan temporary canvas untuk crop
        const tempCanvas = document.createElement('canvas');
        const tempCtx = tempCanvas.getContext('2d');
        tempCanvas.width = canvas.width;
        // Enable image smoothing untuk kualitas lebih baik
        tempCtx.imageSmoothingEnabled = true;
        tempCtx.imageSmoothingQuality = 'high';
        
        let sourceY = 0; // Offset dalam pixels dari gambar asli
        const overlapPixels = 100; // Overlap BESAR untuk memastikan tidak ada konten yang terpotong
        
        for (let pageNum = 1; pageNum <= totalPages; pageNum++) {
          // Jika bukan halaman pertama, tambahkan halaman baru
          if (pageNum > 1) {
            pdf.addPage();
          }
          
          // Set currentY setelah header
          let currentY = titleHeight;
          
          // Tambahkan header ke setiap halaman (termasuk halaman pertama)
          addPageHeader(pdf, pageNum, totalPages);
          
          // Hitung tinggi gambar untuk halaman ini dalam mm
          // Gunakan perhitungan yang SANGAT KONSERVATIF untuk mencegah potongan
          const remainingHeight = imgHeight - ((pageNum - 1) * maxHeightPerPage);
          
          // Kurangi tinggi setiap halaman dengan buffer untuk memastikan tidak ada yang terpotong
          const bufferMm = 5; // Buffer 5mm untuk setiap halaman
          let pageImgHeight = Math.min(maxHeightPerPage - bufferMm, remainingHeight);
          
          // Untuk halaman setelah pertama, tambahkan overlap
          if (pageNum > 1) {
            const overlapMm = (overlapPixels / (canvas.height / imgHeight)); // Convert overlap ke mm
            // Pastikan pageImgHeight tidak terlalu besar
            pageImgHeight = Math.min(maxHeightPerPage - bufferMm, remainingHeight + overlapMm - bufferMm);
          }
          
          // Pastikan pageImgHeight tidak negatif atau terlalu kecil
          if (pageImgHeight < 10) {
            pageImgHeight = Math.min(remainingHeight, maxHeightPerPage);
          }
          
          const pageImgWidth = imgWidth;
          
          // Hitung proporsi yang akan di-crop dalam pixels
          // Gunakan proporsi yang tepat antara mm dan pixels
          const pixelsPerMm = canvas.height / imgHeight; // Pixels per mm
          let pixelsForThisPage = pageImgHeight * pixelsPerMm;
          
          // Untuk halaman setelah pertama, tambahkan overlap di awal
          let actualSourceY = sourceY;
          if (pageNum > 1) {
            const overlapSource = Math.floor(overlapPixels);
            if (sourceY >= overlapSource) {
              actualSourceY = sourceY - overlapSource; // Mulai lebih awal untuk overlap
              pixelsForThisPage += overlapSource; // Tambahkan tinggi untuk overlap
            }
          }
          
          // Hitung tinggi dalam pixels untuk crop - pastikan tidak melebihi canvas height
          // Gunakan perhitungan yang SANGAT KONSERVATIF dengan buffer tambahan
          const maxAvailableHeight = canvas.height - actualSourceY;
          let sourceHeight = Math.min(
            Math.ceil(pixelsForThisPage),
            maxAvailableHeight // Pastikan tidak melebihi sisa canvas
          );
          
          // Tambahkan buffer pixel untuk memastikan tidak ada yang terpotong
          const pixelBuffer = 30; // Buffer 30 pixels
          sourceHeight = Math.min(sourceHeight, maxAvailableHeight - pixelBuffer);
          
          // Pastikan actualSourceY tidak negatif
          if (actualSourceY < 0) {
            sourceHeight += actualSourceY; // Kurangi sourceHeight dengan nilai negatif
            actualSourceY = 0;
          }
          
          // Pastikan sourceHeight valid dan tidak negatif atau terlalu kecil
          if (sourceHeight <= pixelBuffer || actualSourceY >= canvas.height) {
            console.warn(`Page ${pageNum}: No more content to render. actualSourceY: ${actualSourceY}, canvas.height: ${canvas.height}, sourceHeight: ${sourceHeight}`);
            break;
          }
          
          // Pastikan actualSourceY + sourceHeight tidak melebihi canvas.height
          if (actualSourceY + sourceHeight > canvas.height) {
            // Sesuaikan sourceHeight agar tidak melebihi canvas dengan buffer
            const adjustedSourceHeight = canvas.height - actualSourceY - pixelBuffer;
            if (adjustedSourceHeight > pixelBuffer) {
              sourceHeight = adjustedSourceHeight;
            } else {
              // Jika sisa terlalu kecil, gunakan semua yang tersisa
              sourceHeight = canvas.height - actualSourceY;
              if (sourceHeight <= 0) {
                break; // Tidak ada lagi konten
              }
            }
          }
          
          // Validasi final: pastikan sourceHeight minimal
          if (sourceHeight < 50) {
            // Jika terlalu kecil, mungkin ini halaman terakhir, gunakan semua yang tersisa
            sourceHeight = Math.min(sourceHeight, canvas.height - actualSourceY);
            if (sourceHeight <= 0) {
              break;
            }
          }
          
          // Gunakan actualSourceY untuk draw
          const drawSourceY = actualSourceY;
          
          tempCanvas.height = sourceHeight;
          
          // Clear canvas
          tempCtx.clearRect(0, 0, tempCanvas.width, tempCanvas.height);
          
          // Enable image smoothing untuk kualitas lebih baik
          tempCtx.imageSmoothingEnabled = true;
          tempCtx.imageSmoothingQuality = 'high';
          
          // Draw bagian gambar yang sesuai ke temporary canvas (crop)
          tempCtx.drawImage(
            canvas,
            0, drawSourceY, canvas.width, sourceHeight, // source rectangle - gunakan drawSourceY
            0, 0, canvas.width, sourceHeight // destination rectangle
          );
          
          // Convert temporary canvas ke image data dengan kualitas maksimal
          const pageImgData = tempCanvas.toDataURL('image/png', 1.0);
          
          // Tambahkan gambar ke PDF dengan kualitas tinggi (bukan 'FAST')
          pdf.addImage(pageImgData, 'PNG', margin, currentY, pageImgWidth, pageImgHeight, undefined, 'SLOW');
          
          // Update sourceY untuk halaman berikutnya - pastikan tidak melebihi canvas height
          // Untuk halaman dengan overlap, hanya maju sejauh yang diperlukan (kurangi overlap)
          if (pageNum > 1 && pageNum < totalPages) {
            // Untuk halaman tengah, maju dengan mengurangi overlap yang sudah ditambahkan
            const overlapSource = Math.floor(overlapPixels);
            sourceY = drawSourceY + sourceHeight - overlapSource; // Kurangi overlap
          } else if (pageNum === 1) {
            // Halaman pertama, maju normal
            sourceY = drawSourceY + sourceHeight;
          } else {
            // Halaman terakhir, maju sampai akhir
            sourceY = drawSourceY + sourceHeight;
          }
          
          // Pastikan sourceY tidak melebihi canvas height
          if (sourceY >= canvas.height) {
            break; // Semua konten sudah di-render
          }
          
          // Pastikan sourceY tidak mundur (harus selalu maju)
          if (sourceY <= drawSourceY) {
            sourceY = drawSourceY + sourceHeight; // Pastikan selalu maju
          }
        }
        
        // Cleanup temporary canvas
        tempCanvas.width = 0;
        tempCanvas.height = 0;

        // Save PDF
        pdf.save(`riwayat-capaian-${new Date().getTime()}.pdf`);

        // PASTIKAN chart asli masih ada dan tidak terpengaruh
        // Chart asli TIDAK pernah dihapus, jadi seharusnya masih ada
        const checkOriginalChart = document.getElementById('barChart');
        if (checkOriginalChart) {
          const checkChartInstance = Chart.getChart('barChart');
          if (!checkChartInstance) {
            // Instance hilang, render ulang
            if (filteredData.length > 0) {
              createChart();
            }
          } else {
            // Pastikan chart tetap terlihat dengan update
            checkChartInstance.update('none');
          }
        }

        // Restore button
        btn.disabled = false;
        btn.innerHTML = originalText;

        alert('Halaman berhasil diekspor ke PDF!');
      } catch (error) {
        console.error('Error exporting page:', error);
        let errorMessage = 'Gagal mengekspor halaman ke PDF.';
        
        if (error.message) {
          if (error.message.includes('timeout')) {
            errorMessage = 'Export timeout: Proses terlalu lama. Silakan coba lagi atau kurangi jumlah data.';
          } else if (error.message.includes('Canvas')) {
            errorMessage = 'Error pada canvas: ' + error.message;
          } else if (error.message.includes('memory')) {
            errorMessage = 'Memory tidak cukup. Silakan tutup aplikasi lain dan coba lagi.';
          } else {
            errorMessage = error.message;
          }
        }
        
        alert(errorMessage + '\n\nDetail error: ' + (error.stack || error.toString()));
        
        // Cleanup if container still exists
        if (exportContainer && exportContainer.parentNode) {
          exportContainer.parentNode.removeChild(exportContainer);
        }
        
        // PASTIKAN chart asli masih ada setelah error
        const checkOriginalChart = document.getElementById('barChart');
        if (checkOriginalChart) {
          const checkChartInstance = Chart.getChart('barChart');
          if (!checkChartInstance) {
            // Instance hilang, render ulang
            if (filteredData.length > 0) {
              createChart();
            }
          } else {
            // Pastikan chart tetap terlihat dengan update
            checkChartInstance.update('none');
          }
        }
        
        // Restore button
        if (btn) {
          btn.disabled = false;
          btn.innerHTML = originalText;
        }
      }
    }
  </script>

  <script>
    function toggleSidebar() {
      const sidebar = document.getElementById('sidebar');
      sidebar.classList.toggle('open');
    }

    // Update active menu item based on current page
    document.addEventListener('DOMContentLoaded', function() {
      const currentPath = window.location.pathname;
      const menuItems = document.querySelectorAll('.menu-item');
      
      menuItems.forEach(item => {
        const href = item.getAttribute('href');
        if (href === currentPath) {
          item.classList.add('active');
        } else {
          item.classList.remove('active');
        }
      });
    });

    // Close sidebar when clicking outside on mobile
    document.addEventListener('click', function(event) {
      const sidebar = document.getElementById('sidebar');
      const toggleBtn = document.querySelector('.mobile-menu-toggle');
      
      if (window.innerWidth <= 768) {
        if (sidebar && toggleBtn && !sidebar.contains(event.target) && !toggleBtn.contains(event.target) && sidebar.classList.contains('open')) {
          sidebar.classList.remove('open');
        }
      }
    });

    // Tour Guide Script
    const tourIcons = {
      menu: '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="3" y1="12" x2="21" y2="12"></line><line x1="3" y1="6" x2="21" y2="6"></line><line x1="3" y1="18" x2="21" y2="18"></line></svg>',
      history: '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z"></path><polyline points="14 2 14 8 20 8"></polyline><line x1="16" y1="13" x2="8" y2="13"></line><line x1="16" y1="17" x2="8" y2="17"></line></svg>',
      table: '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="3" y="3" width="18" height="18" rx="2"></rect><line x1="3" y1="9" x2="21" y2="9"></line><line x1="9" y1="21" x2="9" y2="9"></line></svg>',
      filter: '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polygon points="22 3 2 3 10 12.46 10 19 14 21 14 12.46 22 3"></polygon></svg>',
      export: '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path><polyline points="7 10 12 15 17 10"></polyline><line x1="12" y1="15" x2="12" y2="3"></line></svg>',
      search: '<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><circle cx="11" cy="11" r="8"></circle><path d="M21 21l-4.35-4.35"></path></svg>'
    };

    function getAvailableTourSteps() {
      const allSteps = [
        {
          element: '#sidebar',
          title: 'Menu Navigasi',
          content: 'Ini adalah menu navigasi utama. Gunakan menu ini untuk berpindah antar halaman seperti Dashboard, Riwayat Capaian, Upload Data, dan lainnya.',
          iconKey: 'menu',
          position: 'right'
        },
        {
          element: '.filter-section',
          title: 'Filter Data',
          content: 'Gunakan filter ini untuk mencari data berdasarkan Fakultas, Program Studi, atau Tahun. Filter membantu Anda menemukan data spesifik dengan cepat.',
          iconKey: 'filter',
          position: 'top'
        },
        {
          element: 'input[type="text"][placeholder*="Cari"]',
          title: 'Pencarian',
          content: 'Kotak pencarian ini memungkinkan Anda mencari data berdasarkan nama fakultas atau program studi. Ketik kata kunci untuk memfilter hasil.',
          iconKey: 'search',
          position: 'top'
        },
        {
          element: '.data-table',
          title: 'Tabel Riwayat Capaian',
          content: 'Tabel ini menampilkan riwayat capaian IKU-1 per fakultas dan program studi. Setiap baris menampilkan data capaian dengan progress bar visual.',
          iconKey: 'table',
          position: 'top'
        },
        {
          element: '.progress-bar',
          title: 'Progress Bar Capaian',
          content: 'Progress bar menunjukkan persentase capaian IKU-1 secara visual. Warna hijau = mencapai target, kuning = mendekati target, merah = perlu peningkatan.',
          iconKey: 'history',
          position: 'top'
        },
        {
          element: 'button[onclick*="export"]',
          title: 'Export Data',
          content: 'Tombol ini digunakan untuk mengekspor data riwayat capaian ke format Excel. Berguna untuk analisis lebih lanjut atau pelaporan.',
          iconKey: 'export',
          position: 'top'
        }
      ];

      return allSteps.filter(step => {
        const element = document.querySelector(step.element);
        return element !== null;
      });
    }

    let currentTourStep = 0;
    let tourActive = false;
    let tourSteps = [];

    function startTour() {
      const tourCompleted = localStorage.getItem('tourCompleted');
      if (tourCompleted === 'true') {
        if (!confirm('Apakah Anda ingin melihat panduan lagi?')) {
          return;
        }
      }

      tourSteps = getAvailableTourSteps();
      
      if (tourSteps.length === 0) {
        alert('Tidak ada elemen yang dapat ditampilkan dalam panduan.');
        return;
      }

      tourActive = true;
      currentTourStep = 0;
      
      const overlay = document.getElementById('tourOverlay');
      overlay.style.display = 'block';
      setTimeout(() => {
        overlay.classList.add('active');
      }, 10);
      
      showTourStep(0);
    }

    function showTourStep(stepIndex) {
      if (stepIndex < 0 || stepIndex >= tourSteps.length) {
        endTour();
        return;
      }

      const step = tourSteps[stepIndex];
      const element = document.querySelector(step.element);

      if (!element) {
        if (stepIndex < tourSteps.length - 1) {
          showTourStep(stepIndex + 1);
        } else {
          endTour();
        }
        return;
      }

      const iconElement = document.getElementById('tourIcon');
      iconElement.innerHTML = tourIcons[step.iconKey] || tourIcons.menu;
      document.getElementById('tourTitle').textContent = step.title;
      document.getElementById('tourContent').textContent = step.content;
      document.getElementById('tourProgress').textContent = `${stepIndex + 1} / ${tourSteps.length}`;

      const prevBtn = document.getElementById('tourPrevBtn');
      const nextBtn = document.getElementById('tourNextBtn');
      
      prevBtn.style.display = stepIndex === 0 ? 'none' : 'flex';
      nextBtn.textContent = stepIndex === tourSteps.length - 1 ? 'Selesai' : 'Selanjutnya';

      const rect = element.getBoundingClientRect();
      const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
      const scrollLeft = window.pageXOffset || document.documentElement.scrollLeft;

      const highlight = document.getElementById('tourHighlight');
      highlight.classList.add('active');
      highlight.style.top = (rect.top + scrollTop - 4) + 'px';
      highlight.style.left = (rect.left + scrollLeft - 4) + 'px';
      highlight.style.width = (rect.width + 8) + 'px';
      highlight.style.height = (rect.height + 8) + 'px';

      const tooltip = document.getElementById('tourTooltip');
      tooltip.style.display = 'block';
      tooltip.className = 'tour-tooltip ' + step.position;

      let tooltipTop, tooltipLeft;
      const tooltipWidth = Math.max(300, tooltip.offsetWidth || 350);
      const tooltipHeight = Math.max(180, tooltip.offsetHeight || 200);
      const spacing = 20;

      switch(step.position) {
        case 'top':
          tooltipTop = rect.top + scrollTop - tooltipHeight - spacing;
          tooltipLeft = rect.left + scrollLeft + (rect.width / 2) - (tooltipWidth / 2);
          break;
        case 'bottom':
          tooltipTop = rect.bottom + scrollTop + spacing;
          tooltipLeft = rect.left + scrollLeft + (rect.width / 2) - (tooltipWidth / 2);
          break;
        case 'left':
          tooltipTop = rect.top + scrollTop + (rect.height / 2) - (tooltipHeight / 2);
          tooltipLeft = rect.left + scrollLeft - tooltipWidth - spacing;
          break;
        case 'right':
          tooltipTop = rect.top + scrollTop + (rect.height / 2) - (tooltipHeight / 2);
          tooltipLeft = rect.right + scrollLeft + spacing;
          break;
        default:
          tooltipTop = rect.bottom + scrollTop + spacing;
          tooltipLeft = rect.left + scrollLeft + (rect.width / 2) - (tooltipWidth / 2);
      }

      const viewportWidth = window.innerWidth;
      const viewportHeight = window.innerHeight;

      if (tooltipLeft < 10) tooltipLeft = 10;
      if (tooltipLeft + tooltipWidth > viewportWidth - 10) {
        tooltipLeft = viewportWidth - tooltipWidth - 10;
      }
      if (tooltipTop < 10) tooltipTop = 10;
      if (tooltipTop + tooltipHeight > viewportHeight - 10) {
        tooltipTop = viewportHeight - tooltipHeight - 10;
      }

      tooltip.style.top = tooltipTop + 'px';
      tooltip.style.left = tooltipLeft + 'px';

      element.scrollIntoView({ behavior: 'smooth', block: 'center' });
      highlight.style.animation = 'pulse 2s infinite';
    }

    function nextTourStep() {
      if (currentTourStep < tourSteps.length - 1) {
        currentTourStep++;
        showTourStep(currentTourStep);
      } else {
        endTour();
      }
    }

    function prevTourStep() {
      if (currentTourStep > 0) {
        currentTourStep--;
        showTourStep(currentTourStep);
      }
    }

    function skipTour() {
      endTour();
    }

    function endTour() {
      tourActive = false;
      const overlay = document.getElementById('tourOverlay');
      overlay.classList.remove('active');
      setTimeout(() => {
        overlay.style.display = 'none';
      }, 300);
      const highlight = document.getElementById('tourHighlight');
      highlight.classList.remove('active');
      highlight.style.display = 'none';
      document.getElementById('tourTooltip').style.display = 'none';
      localStorage.setItem('tourCompleted', 'true');
    }

    const style = document.createElement('style');
    style.textContent = `
      @keyframes pulse {
        0%, 100% {
          box-shadow: 0 0 0 9999px rgba(0, 0, 0, 0.7), 0 0 20px rgba(47, 107, 49, 0.5);
        }
        50% {
          box-shadow: 0 0 0 9999px rgba(0, 0, 0, 0.7), 0 0 30px rgba(47, 107, 49, 0.8);
        }
      }
    `;
    document.head.appendChild(style);

    document.getElementById('tourOverlay').addEventListener('click', function(e) {
      if (e.target === this) {
        endTour();
      }
    });

    document.addEventListener('keydown', function(e) {
      if (!tourActive) return;
      
      if (e.key === 'Escape') {
        endTour();
      } else if (e.key === 'ArrowRight' || e.key === 'Enter') {
        nextTourStep();
      } else if (e.key === 'ArrowLeft') {
        prevTourStep();
      }
    });
  </script>
</body>
