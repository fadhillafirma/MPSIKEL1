  <script>
    // Global variables
    let chartInstance = null;
    let chartType = 'bar';
    let originalData = JSON.parse('<%- JSON.stringify(data || []) %>');
    let filteredData = [...originalData];

    // Display current date
    const dateElement = document.getElementById("currentDate");
    const today = new Date();
    const options = { weekday: 'long', year: 'numeric', month: 'short', day: 'numeric' };
    dateElement.textContent = today.toLocaleDateString('id-ID', options);

    document.addEventListener('DOMContentLoaded', () => {
      initializeStats();
      initializeFilters();
      initializeChart();
      initializeTable();
      initializeProgressBars();
      const tahunFilter = document.getElementById('tahunFilter');
      if (tahunFilter) {
        tahunFilter.addEventListener('change', applyFilters);
      }
    });

    function initializeStats() {
      const stats = JSON.parse('<%- JSON.stringify(stats || {}) %>');
      document.getElementById('totalAlumni').textContent = (stats.totalAlumni || 0).toLocaleString();
      // Format rata-rata capaian dengan 2 desimal (sama seperti dashboard)
      const avgAchievement = parseFloat(stats.avgAchievement || 0);
      document.getElementById('avgAchievement').textContent = avgAchievement.toFixed(2) + '%';
      document.getElementById('totalProdi').textContent = stats.totalProdi || 0;
    }

    function initializeProgressBars() {
      document.querySelectorAll('.progress-fill[data-width]').forEach(el => {
        el.style.width = el.dataset.width + '%';
      });
    }

    function initializeFilters() {
      // Fakultas sudah dirender dari server, cukup attach event listener
      const fakultasFilter = document.getElementById('fakultasFilter');
      fakultasFilter.addEventListener('change', applyFilters);
      document.getElementById('statusFilter').addEventListener('change', applyFilters);
    }

    function applyFilters() {
      const fakultas = document.getElementById('fakultasFilter').value;
      const tahun = document.getElementById('tahunFilter').value;
      const status = document.getElementById('statusFilter').value;

      filteredData = originalData.filter(i => {
        return (!fakultas || i.fakultas === fakultas) &&
               (!tahun || String(i.tahun_lulus) === tahun) &&
               (!status || i.status === status);
      });
      updateChart();
      updateTable();
    }

    function resetFilters() {
      document.getElementById('fakultasFilter').value = '';
      document.getElementById('tahunFilter').value = '';
      document.getElementById('statusFilter').value = '';
      filteredData = [...originalData];
      updateChart();
      updateTable();
    }

    function initializeChart() {
      if (filteredData.length > 0) createChart();
    }

    function createChart() {
      const ctx = document.getElementById('barChart');
      if (chartInstance) chartInstance.destroy();
      const labels = filteredData.map(d => d.prodi);
      const dataValues = filteredData.map(d => parseFloat(d.capaian_rata));
      const colors = dataValues.map(v => v >= 75 ? '#28a745' : v >= 50 ? '#ffc107' : '#dc3545');
      
      chartInstance = new Chart(ctx, {
        type: chartType,
        data: {
          labels,
          datasets: [{
            data: dataValues,
            backgroundColor: colors,
            borderColor: colors,
            borderWidth: 1,
            borderRadius: 5
          }]
        },
        options: {
          responsive: true,
          maintainAspectRatio: false,
          plugins: {
            legend: { display: false }
          },
          scales: {
            y: {
              beginAtZero: true,
              max: 100,
              ticks: { color: '#555' }
            },
            x: { ticks: { color: '#555' } }
          }
        }
      });
    }

    function toggleChartType() {
      chartType = chartType === 'bar' ? 'line' : 'bar';
      updateChart();
    }

    function updateChart() {
      if (chartInstance) chartInstance.destroy();
      if (filteredData.length > 0) createChart();
    }

    function updateTable() {
      const tbody = document.getElementById('tableBody');
      tbody.innerHTML = '';
      
      if (filteredData.length === 0) {
        tbody.innerHTML = '<tr><td colspan="5" class="no-data">Tidak ada data.</td></tr>';
        return;
      }
      
      filteredData.forEach(item => {
        const jumlahAlumni = item.jumlah_alumni || 0;
        // Pastikan capaian_rata selalu di-parse dengan benar
        let capaianRata = 0;
        if (jumlahAlumni > 0 && item.capaian_rata !== null && item.capaian_rata !== undefined && item.capaian_rata !== '') {
          const parsed = parseFloat(item.capaian_rata);
          capaianRata = isNaN(parsed) ? 0 : parsed;
        }
        
        const colorClass = jumlahAlumni === 0 ? '' : (capaianRata >= 75 ? 'green' : capaianRata >= 50 ? 'yellow' : 'red');
        const statusClass = jumlahAlumni === 0 ? '' : (capaianRata >= 75 ? 'success' : capaianRata >= 50 ? 'warning' : 'danger');
        const statusText = jumlahAlumni === 0 ? '-' : (capaianRata >= 75 ? 'Mencapai Target' : capaianRata >= 50 ? 'Mendekati Target' : 'Perlu Peningkatan');
        
        // Pastikan status badge selalu ada
        const statusBadge = jumlahAlumni === 0 
          ? '<span class="status-badge">-</span>' 
          : `<span class="status-badge status-${statusClass}">${statusText}</span>`;
        const row = `
          <tr data-fakultas="${item.fakultas}" data-prodi="${item.prodi}" data-persentase="${capaianRata}">
            <td>${item.fakultas}</td>
            <td>${item.prodi}</td>
            <td>
              <div class="progress-bar">
                <div class="progress-fill ${colorClass}" data-width="${capaianRata}"></div>
                <span class="progress-text">${capaianRata}%</span>
              </div>
            </td>
            <td>${jumlahAlumni}</td>
            <td>${statusBadge}</td>
          </tr>`;
        tbody.insertAdjacentHTML('beforeend', row);
      });
      
      setTimeout(() => {
        document.querySelectorAll('.progress-fill[data-width]').forEach(el => {
          el.style.width = el.dataset.width + '%';
        });
      }, 0);
    }

    function exportData() {
      const headers = ['Fakultas', 'Program Studi', 'Capaian (%)', 'Jumlah Alumni', 'Status'];
      const rows = filteredData.map(i => [i.fakultas, i.prodi, i.capaian_rata, i.jumlah_alumni || 0, i.status]);
      const csv = [headers, ...rows].map(r => r.join(',')).join('\n');
      const blob = new Blob([csv], { type: 'text/csv' });
      const link = document.createElement('a');
      link.href = URL.createObjectURL(blob);
      link.download = 'capaian-iku1.csv';
      link.click();
    }

    function refreshData() {
      location.reload();
    }

    // Function untuk sorting tabel - harus di global scope agar bisa diakses dari inline handler
    window.sortTable = function sortTable() {
      const sortSelect = document.getElementById('sortSelect');
      if (!sortSelect) {
        console.error('sortSelect tidak ditemukan');
        return;
      }
      
      const sortValue = sortSelect.value;
      const tbody = document.getElementById('tableBody');
      if (!tbody) {
        console.error('tableBody tidak ditemukan');
        return;
      }
      
      const rows = Array.from(tbody.querySelectorAll('tr'));
      
      // Hapus row "no data" jika ada
      const noDataRow = rows.find(row => row.querySelector('.no-data'));
      if (noDataRow) {
        noDataRow.remove();
      }

      // Jika tidak ada sorting yang dipilih, reset ke data filtered
      if (!sortValue) {
        updateTable();
        return;
      }

      // Filter rows yang valid (bukan no-data dan memiliki data attributes)
      const validRows = rows.filter(row => {
        const hasNoData = row.querySelector('.no-data');
        const hasData = row.hasAttribute('data-fakultas') || row.hasAttribute('data-prodi') || row.hasAttribute('data-persentase');
        return !hasNoData && hasData;
      });
      
      if (validRows.length === 0) {
        console.warn('Tidak ada rows yang valid untuk di-sort');
        return;
      }

      // Sort rows berdasarkan pilihan
      validRows.sort((a, b) => {
        let aValue, bValue;
        
        if (sortValue.startsWith('persentase')) {
          // Sorting berdasarkan persentase
          aValue = parseFloat(a.getAttribute('data-persentase') || 0);
          bValue = parseFloat(b.getAttribute('data-persentase') || 0);
          
          if (sortValue === 'persentase-desc') {
            return bValue - aValue; // Tertinggi ke terendah
          } else {
            return aValue - bValue; // Terendah ke tertinggi
          }
        } else if (sortValue.startsWith('fakultas')) {
          // Sorting berdasarkan fakultas
          aValue = (a.getAttribute('data-fakultas') || '').toLowerCase();
          bValue = (b.getAttribute('data-fakultas') || '').toLowerCase();
          
          if (sortValue === 'fakultas-desc') {
            return bValue.localeCompare(aValue, 'id'); // Z-A
          } else {
            return aValue.localeCompare(bValue, 'id'); // A-Z
          }
        } else if (sortValue.startsWith('prodi')) {
          // Sorting berdasarkan program studi
          aValue = (a.getAttribute('data-prodi') || '').toLowerCase();
          bValue = (b.getAttribute('data-prodi') || '').toLowerCase();
          
          if (sortValue === 'prodi-desc') {
            return bValue.localeCompare(aValue, 'id'); // Z-A
          } else {
            return aValue.localeCompare(bValue, 'id'); // A-Z
          }
        }
        
        return 0;
      });

      // Clear tbody dan append rows yang sudah di-sort
      tbody.innerHTML = '';
      validRows.forEach(row => tbody.appendChild(row));
      
      // Re-initialize progress bars setelah sorting
      setTimeout(() => {
        initializeProgressBars();
      }, 0);
    };

    function initializeTable() {
      updateTable();
    }

    // Auto refresh data every 5 seconds
    async function autoRefreshDashboard() {
      try {
        const res = await fetch('/api/data');
        const json = await res.json();
        if (!json.success) return;

        document.getElementById('totalAlumni').textContent = (json.stats.totalAlumni || 0).toLocaleString();
        // Format rata-rata capaian dengan 2 desimal (sama seperti dashboard)
        const avgAchievement = parseFloat(json.stats.avgAchievement || 0);
        document.getElementById('avgAchievement').textContent = avgAchievement.toFixed(2) + '%';
        document.getElementById('totalProdi').textContent = json.stats.totalProdi || 0;

        originalData = json.data;
        filteredData = [...originalData];
        updateChart();
        updateTable();
        initializeFilters();
      } catch (err) {
        console.error('Gagal memuat data otomatis:', err);
      }
    }

    // Uncomment to enable auto-refresh
    // setInterval(autoRefreshDashboard, 5000);

    // Export Chart Function - Export individual chart as PNG
    function exportChart(canvasId, fileName) {
      try {
        const canvas = document.getElementById(canvasId);
        if (!canvas) {
          alert('Chart tidak ditemukan!');
          return;
        }

        // Get chart instance
        const chart = Chart.getChart(canvasId);
        if (!chart) {
          alert('Chart belum dimuat!');
          return;
        }

        // Export chart as PNG using Chart.js built-in method
        const url = chart.toBase64Image('image/png', 1);
        
        // Create download link
        const link = document.createElement('a');
        link.download = `${fileName || 'chart'}-${new Date().getTime()}.png`;
        link.href = url;
        link.click();

        // Show success message
        console.log('Chart berhasil diekspor!');
      } catch (error) {
        console.error('Error exporting chart:', error);
        alert('Gagal mengekspor chart. Silakan coba lagi.');
      }
    }

    // Export Full Page as PDF
    async function exportFullPage() {
      const btn = event.target.closest('button');
      const originalText = btn.innerHTML;
      let exportContainer = null;
      
